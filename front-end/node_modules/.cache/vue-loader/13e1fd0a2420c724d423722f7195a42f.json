 if (res.data.code === 200) {\r\n            this.$message({\r\n              message: '删除成功，3秒后消失',\r\n              type: 'success'\r\n            })\r\n          }\r\n          this.getAll()\r\n        })\r\n    },\r\n    edit (id) {\r\n      this.$router.push(`/admin/user/edituser/${id}`)\r\n    },\r\n    // isShow (Identity) {\r\n    //   if (Identity === 1) {\r\n    //     return true\r\n    //   }\r\n    // }\r\n  },\r\n\r\n  created () {\r\n    this.getAll()\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nbody {\r\n  margin: 0;\r\n}\r\n.demo-table-expand .el-form-item {\r\n  margin-right: 0;\r\n  margin-bottom: 0;\r\n  /* width: 50%; */\r\n  color: peru;\r\n}\r\n</style>"]}]}