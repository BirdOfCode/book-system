A;AAoBA,IAAA,YApBA,wBAoBA,WApBA,EAoBA;AACA,aAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,WAAA,GAAA,OAAA,CAAA,WAAA,CAAA,WAAA,EAAA,MAAA,CAAA;AACA,OAFA;AAGA;AAxBA,GAVA;AAoCA,EAAA,OApCA,qBAoCA;AAAA;;AACA,IAAA,UAAA,GACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,MAAA,GAAA,GAAA,GAAA,CAAA,IAAA;;AACA,UAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AAAA;AAAA;AAAA;;AAAA;AACA,+BAAA,GAAA,CAAA,IAAA,8HAAA;AAAA,gBAAA,CAAA;;AACA,YAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AAAA,cAAA,KAAA,EAAA,CAAA,CAAA;AAAA,aAAA;AACA;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA;AACA,KARA;AASA;AA9CA,CAAA","sourcesContent":[" <template>\r\n  <div>\r\n    <el-container style=\"border: 1px solid #eee\">\r\n      <el-header style=\"font-size: 12px\">\r\n        <el-row class=\"demo-autocomplete\"\r\n                :gutter=\"20\">\r\n          <el-col :span=\"4\">\r\n            <i class=\"el-icon-collection\">图书推荐系统</i>\r\n          </el-col>\r\n          <el-col :span=\"16\">\r\n\r\n            <div class=\"grid-content bg-purple\">\r\n              <el-autocomplete class=\"inline-input\"\r\n                               style=\"width:30vw\"\r\n                               v-model=\"state\"\r\n                               :fetch-suggestions=\"querySearch\"\r\n                               placeholder=\"请输入内容\"\r\n                               :trigger-on-focus=\"false\">\r\n                <el-button slot=\"append\"\r\n                           icon=\"el-icon-search\"\r\n                           @click=\"search\"></el-button>\r\n              </el-autocomplete>\r\n            </div>\r\n          </el-col>\r\n\r\n          <el-col :span=\"4\">\r\n            <div class=\"grid-content bg-purple\"\r\n                 style=\"font-size: 14px\">\r\n              <el-dropdown @command=\"out\">\r\n                <i class=\"el-icon-setting\"\r\n                   style=\"margin-right: 10px\"></i>\r\n                <el-dropdown-menu slot=\"dropdown\"\r\n                                  @click=\"out\">\r\n                  <el-dropdown-item>退出</el-dropdown-item>\r\n                </el-dropdown-menu>\r\n              </el-dropdown>\r\n              <span>{{$store.state.username}}</span>\r\n            </div>\r\n          </el-col>\r\n\r\n        </el-row>\r\n      </el-header>\r\n      <el-container class=\"tabs\">\r\n        <el-main>\r\n          <el-tabs tab-position='top'>\r\n            <el-tab-pane label=\"Hot\"\r\n                         lazy>\r\n              <books kind='hot'></books>\r\n            </el-tab-pane>\r\n            <el-tab-pane label=\"文学\"\r\n                         lazy>\r\n              <books kind='literature'></books>\r\n            </el-tab-pane>\r\n            <el-tab-pane label=\"流行\"\r\n                         lazy>\r\n              <books kind='popular'></books>\r\n            </el-tab-pane>\r\n            <el-tab-pane label=\"文化\"\r\n                         lazy>\r\n              <books kind='culture'></books>\r\n            </el-tab-pane>\r\n            <el-tab-pane label=\"生活\"\r\n                         lazy>\r\n              <books kind='life'></books>\r\n            </el-tab-pane>\r\n            <el-tab-pane label=\"经管\"\r\n                         lazy>\r\n              <books kind='manage'></books>\r\n            </el-tab-pane>\r\n            <el-tab-pane label=\"科技\"\r\n                         lazy>\r\n              <books kind='technology'></books>\r\n            </el-tab-pane>\r\n          </el-tabs>\r\n        </el-main>\r\n      </el-container>\r\n    </el-container>\r\n  </div>\r\n</template>\r\n\r\n<style>\r\nhtml {\r\n  overflow: scroll;\r\n}\r\n\r\nbody {\r\n  background: #eeeeee;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.tabs {\r\n  margin: 0 auto;\r\n  width: 65vw;\r\n}\r\n.el-header {\r\n  background-color: #b3c0d1;\r\n  color: #333;\r\n  line-height: 60px;\r\n}\r\n\r\n.el-aside {\r\n  color: #333;\r\n}\r\n.el-icon-collection {\r\n  margin: 10px 10px;\r\n  font-size: 25px;\r\n  /* vertical-align: middle; */\r\n}\r\n</style>\r\n\r\n<script>\r\nimport books from '../user/books'\r\nimport { getAllBook } from '../../network/admin'\r\nimport { search } from '../../network/user'\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      books: [],\r\n      state: '',\r\n    };\r\n  },\r\n  components: {\r\n    books\r\n  },\r\n  methods: {\r\n    search () {\r\n      search(this.state)\r\n        .then(res => {\r\n          if (res.data.code === 200) {\r\n            window.open(res.data.data.detail, '_blank')\r\n          }\r\n        })\r\n    },\r\n    out () {\r\n      localStorage.clear();\r\n      this.$router.push('/login')\r\n    },\r\n\r\n    querySearch (queryString, cb) {\r\n      var books = this.books;\r\n      var results = queryString ? books.filter(this.createFilter(queryString)) : books;\r\n      // 调用 callback 返回建议列表的数据\r\n      cb(results);\r\n    },\r\n    createFilter (queryString) {\r\n      return (book) => {\r\n        return (book.value.toLowerCase().indexOf(queryString.toLowerCase()) === 0);\r\n      };\r\n    },\r\n  },\r\n  created () {\r\n    getAllBook()\r\n      .then(res => {\r\n        res = res.data\r\n        if (res.code === 200) {\r\n          for (let i of res.data) {\r\n            this.books.push({ value: i.name })\r\n          }\r\n        }\r\n      })\r\n  },\r\n\r\n\r\n}\r\n</script>"],"sourceRoot":"src/views/user"}]}