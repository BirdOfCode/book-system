AQA;AAlCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <ul>\r\n      <li class=\"box\"\r\n          v-for=\"(item,index) in books\"\r\n          :key=\"index\">\r\n        <div>\r\n          <el-image style=\"width:50%;height:200px;float:left\"\r\n                    :src='item.cover'\r\n                    fit=\"fill\"></el-image>\r\n          <div class=\"info\">\r\n            <p>{{item.name}}</p>\r\n            <p>{{item.author}}</p>\r\n            <p>{{item.price}}</p>\r\n            <p>{{item.ISBN}}</p>\r\n          </div>\r\n          <div>\r\n            <a :href=\"item.detail\"\r\n               target=\"_blank\">\r\n              <el-button type=\"info\"\r\n                         round\r\n                         class=\"detail\"\r\n                         size=\"small\"\r\n                         @click=\"detail(item.ISBN)\">查看详情</el-button>\r\n            </a>\r\n            <el-button type=\"primary\"\r\n                       round\r\n                       size=\"small\"\r\n                       @click=\"collect(item.ISBN)\">收藏</el-button>\r\n\r\n          </div>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getBook, collect, detail } from '../../network/user';\r\nexport default {\r\n  name: 'books',\r\n  data () {\r\n    return {\r\n      books: []\r\n    }\r\n  },\r\n  props: ['kind'],\r\n  methods: {\r\n    detail (isbn) {\r\n      detail(isbn, this.$store.state.username)\r\n    },\r\n    collect (isbn) {\r\n      collect(isbn, this.$store.state.username)\r\n        .then(res => {\r\n          if (res.data.code === 200) {\r\n            this.$message({\r\n              message: '收藏成功，3秒后消失',\r\n              type: 'success'\r\n            })\r\n          }\r\n\r\n        })\r\n    }\r\n  },\r\n  created () {\r\n    getBook(this.kind)\r\n      .then(res => {\r\n        this.books = res.data.data\r\n      },\r\n        rej => {\r\n          console.log(rej);\r\n\r\n        })\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.detail {\r\n  margin-top: 100px;\r\n  margin-left: 5px;\r\n}\r\n.box {\r\n  width: 30%;\r\n  margin-left: 28px;\r\n  list-style: none;\r\n  float: left;\r\n  margin-bottom: 70px;\r\n}\r\n\r\n.info {\r\n  font-size: 13px;\r\n  margin-left: 10px;\r\n  float: left;\r\n}\r\n</style>"],"sourceRoot":"src/views/user"}]}