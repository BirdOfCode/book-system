A,MAAA,CAAA,MAAA;AACA,OATA;AAUA,KA3BA;AA4BA,IAAA,IA5BA,gBA4BA,EA5BA,EA4BA;AACA,WAAA,OAAA,CAAA,IAAA,gCAAA,EAAA;AACA,KA9BA,CA+BA;AACA;AACA;AACA;AACA;;AAnCA,GATA;AA+CA,EAAA,OA/CA,qBA+CA;AACA,SAAA,MAAA;AACA;AAjDA,CAAA","sourcesContent":["<template>\r\n\r\n  <div>\r\n\r\n    <el-table :data=\"users\"\r\n              stripe>\r\n      <el-table-column type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form label-position=\"left\"\r\n                   class=\"demo-table-expand\">\r\n            <el-form-item label=\"查看过详情的图书ISBN\">\r\n              <span>{{ props.row.detailId}}</span>\r\n            </el-form-item>\r\n            <el-form-item label=\"收藏过的图书ISBN\">\r\n              <span>{{ props.row.collectId}}</span>\r\n            </el-form-item>\r\n            <el-form-item label=\"用户关键词\">\r\n              <span>{{ props.row.keyWords}}</span>\r\n            </el-form-item>\r\n            <el-form-item label=\"创建时间\">\r\n              <span>{{ createTime(props.row.createTime)}}</span>\r\n            </el-form-item>\r\n            <el-form-item label=\"上次修改时间\">\r\n              <span>{{ updataTime(props.row.updateTime)}}</span>\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"username\"\r\n                       label=\"用户名\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"password\"\r\n                       label=\"用户密码\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"Identity\"\r\n                       label=\"用户类别\">\r\n      </el-table-column>\r\n\r\n      <el-table-column fixed=\"right\"\r\n                       label=\"操作\"\r\n                       width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n\r\n          <el-button @click=\"edit(scope.row._id)\"\r\n                     type=\"text\"\r\n                     size=\"small\">修改</el-button>\r\n          <el-button @click=\"del(scope.row._id)\"\r\n                     type=\"text\"\r\n                     size=\"small\">删除</el-button>\r\n\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\nimport { getAllUser, delUser } from '../../network/admin'\r\nexport default {\r\n  name: 'bookList',\r\n  data () {\r\n    return {\r\n      users: [],\r\n      searchId: [],\r\n      show: false\r\n    }\r\n  },\r\n  methods: {\r\n    createTime (time) {\r\n      return new Date(time).toLocaleString()\r\n    },\r\n    updataTime (time) {\r\n      return new Date(time).toLocaleString()\r\n    },\r\n    getAll () {\r\n      getAllUser()\r\n        .then(res => {\r\n          res = res.data\r\n          if (res.code === 200) {\r\n            this.users = res.data\r\n          }\r\n        })\r\n    },\r\n    del (id) {\r\n      delUser(id)\r\n        .then(res => {\r\n          if (res.data.code === 200) {\r\n            this.$message({\r\n              message: '删除成功，3秒后消失',\r\n              type: 'success'\r\n            })\r\n          }\r\n          this.getAll()\r\n        })\r\n    },\r\n    edit (id) {\r\n      this.$router.push(`/admin/user/edituser/${id}`)\r\n    },\r\n    // isShow (Identity) {\r\n    //   if (Identity === 1) {\r\n    //     return true\r\n    //   }\r\n    // }\r\n  },\r\n\r\n  created () {\r\n    this.getAll()\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nbody {\r\n  margin: 0;\r\n}\r\n.demo-table-expand .el-form-item {\r\n  margin-right: 0;\r\n  margin-bottom: 0;\r\n  /* width: 50%; */\r\n  color: peru;\r\n}\r\n</style>"],"sourceRoot":"src/views/admin"}]}